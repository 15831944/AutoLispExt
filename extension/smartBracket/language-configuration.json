{
	"comments": {
		"blockComment": [ ";|", "|;" ],
		"lineComment": ";",
	},
    "brackets": [
		["(", ")"],
		["[", "]"]
    ],
    "autoClosingPairs": [
		{"open": "(", "close": ")"},
		{"open": "[", "close": "]"},
		{"open": "\"","close": "\"", "notIn": ["string"]},
		{"open": "#|", "close": "|#"},
		{"open": ";|", "close": "|;"}
    ],
    "surroundingPairs": [
		["\"", "\""],
		["(", ")"],
		["[","]"],
		["#|", "|#"],
		[";|", "|;"]
	],
	"indentationRules": {
		"increaseIndentPattern": "^(\\(defun)|\\(progn\\b",
		"decreaseIndentPattern": "^((?!.*?\\/\\*).*\\*/)?\\s*[\\}\\]].*$"
	  }
}